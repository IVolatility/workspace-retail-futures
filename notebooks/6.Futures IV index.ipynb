    "import os\n",
    "\n",
    "ivolApiKey = os.getenv('API_KEY')\n",
    "ivol.setLoginParams(apiKey=ivolApiKey)\n",
    "\n",
    "getMarketData = ivol.setMethod(\"/futures/eod/ivx\")\n",
    "\n",
    "tickers = 'ES,GC,CT'\n",
    "\n",
    "marketData = getMarketData(\n",
    "    symbol=tickers, date='2025-04-14'\n",
    ")\n",
    "\n",
    "marketData"
   ]
